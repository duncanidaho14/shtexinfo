<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
            <!--Import materialize.css-->
            {# <link href=" {{ asset('./../node_modules/bootstrap/dist/css/bootstrap.css') }}" rel="stylesheet" /> #}
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.css" />

            {# 'app' must match the first argument to addEntry() in webpack.config.js #}
            
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Dosis:300,400,600" media="all" />

            <!-- Renders a link tag (if your module requires any CSS)-->
                 <link rel="stylesheet" href="{{ asset('./../public/build/app.css')}}" />
        {% endblock %}
    </head>
    <body>
        {% block nav %}
        {% endblock nav %}
        
        {% block header %}
        {% endblock header %}

         {% block body %}
        {% endblock body %}
        
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="{{ asset('./../node_modules/jquery/dist/jquery.js') }}"></script>
            <script src="{{ asset('./../node_modules/popper.js') }}"></script>
            <script src="{{ asset('./../node_modules/bootstrap/dist/js/bootstrap.js') }}"></script>
            <script src="{{ asset('./../public/build/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
